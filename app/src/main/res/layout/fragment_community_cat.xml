<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="android.view.View" />

        <variable
            name="communityCatViewModel"
            type="com.dailylocally.ui.community.catlist.CommunityCatViewModel" />
    </data>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/search"
            android:background="@color/white"
            android:visibility="@{communityCatViewModel.fullEmpty?View.VISIBLE:View.GONE}">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:orientation="vertical">

                <ImageView
                    imageUrl="@{communityCatViewModel.emptyImageUrl}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:scaleType="centerInside" />

                <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                    style="@style/HeaderTitleTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="15dp"
                    android:gravity="center"
                    android:text="@{communityCatViewModel.emptyContent}"
                    android:visibility="gone" />

                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                    style="@style/SubtTitleTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:gravity="center"
                    android:text="@{communityCatViewModel.emptySubContent}"
                    android:visibility="gone" />


            </LinearLayout>


        </RelativeLayout>

        <LinearLayout
            android:layout_marginTop="50dp"
            android:id="@+id/homemain"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:orientation="vertical">

            <androidx.core.widget.NestedScrollView
                android:id="@+id/fullScroll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:overScrollMode="never"
                android:visibility="@{communityCatViewModel.fullEmpty?View.GONE:View.VISIBLE}"
                app:layout_behavior="@string/appbar_scrolling_view_behavior">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                        <RelativeLayout
                            android:visibility="@{communityCatViewModel.isCommunityUser?View.VISIBLE:View.GONE}"
                            android:id="@+id/user"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_alignParentTop="true"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <androidx.cardview.widget.CardView
                                    android:layout_marginBottom="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    app:cardElevation="3dp"
                                    app:cardCornerRadius="30dp">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="30dp"
                                    android:layout_marginBottom="20dp"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_marginEnd="10dp"
                                        android:orientation="horizontal">

                                        <RelativeLayout
                                            android:visibility="gone"
                                            android:layout_width="80dp"
                                            android:layout_height="80dp">

                                            <com.dailylocally.utilities.CircularImageView
                                                android:id="@+id/circleImage"
                                                profileImage="@{communityCatViewModel.profilePic}"
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:onClick="@{()->communityCatViewModel.changeProfile()}"
                                                android:src="@drawable/ic_group_482" />


                                            <ProgressBar
                                                android:layout_centerInParent="true"
                                                android:visibility="@{communityCatViewModel.imageLoader?View.VISIBLE:View.GONE}"
                                                android:layout_width="30dp"
                                                android:layout_height="30dp"/>



                                        </RelativeLayout>

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_vertical"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginEnd="20dp"
                                            android:orientation="vertical">


                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:orientation="horizontal">


                                                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                                    style="@style/SubtTitleTextStyle"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="Hi " />


                                                <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                                                    style="@style/SubtTitleTextStyle"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityCatViewModel.name}" />

                                            </LinearLayout>

                                            <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                                android:layout_marginTop="5dp"
                                                style="@style/SubtTitleTextStyle"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="@{communityCatViewModel.cat_page_content}" />


                                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                                style="@style/ContentTextStyle"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="10dp"
                                                android:text="@{communityCatViewModel.cat_page_subcontent}" />


                                        </LinearLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:visibility="@{communityCatViewModel.isCommunityUser?View.VISIBLE:View.GONE}"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_marginTop="25dp"
                                        android:layout_marginEnd="20dp"
                                        android:orientation="horizontal"
                                        android:weightSum="4">


                                        <RelativeLayout
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1">


                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentTop="true"
                                                android:layout_centerHorizontal="true"
                                                android:gravity="center_horizontal"
                                                android:orientation="vertical">

                                                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityCatViewModel.minValueText}"
                                                    android:textColor="@color/colorPrimary"
                                                    android:textSize="@dimen/_22px" />


                                                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityCatViewModel.minValue}"
                                                    android:textAlignment="center"
                                                    android:textColor="@color/medium_black"
                                                    android:textSize="@dimen/_10px" />


                                            </LinearLayout>


                                        </RelativeLayout>

                                        <RelativeLayout
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="2">


                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentTop="true"
                                                android:layout_centerHorizontal="true"
                                                android:gravity="center_horizontal"
                                                android:orientation="vertical">

                                                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityCatViewModel.freeDeliveryText }"
                                                    android:textColor="@color/colorPrimary"
                                                    android:textSize="@dimen/_22px" />


                                                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityCatViewModel.freeDelivery}"
                                                    android:textAlignment="center"
                                                    android:textColor="@color/medium_black"
                                                    android:textSize="@dimen/_10px" />


                                            </LinearLayout>


                                        </RelativeLayout>

                                        <RelativeLayout
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1">


                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentTop="true"
                                                android:layout_centerHorizontal="true"
                                                android:gravity="center_horizontal"
                                                android:orientation="vertical">

                                                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityCatViewModel.codText }"
                                                    android:textColor="@color/colorPrimary"
                                                    android:textSize="@dimen/_22px" />


                                                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityCatViewModel.cod}"
                                                    android:textAlignment="center"
                                                    android:textColor="@color/medium_black"
                                                    android:textSize="@dimen/_10px" />


                                            </LinearLayout>


                                        </RelativeLayout>
                                    </LinearLayout>


                                </LinearLayout>

                                </androidx.cardview.widget.CardView>
                            </LinearLayout>

                            <View
                                android:focusable="true"
                                android:layout_width="match_parent"
                                android:layout_height="25dp"
                                android:layout_alignParentTop="true"
                                android:background="@color/white" />

                        </RelativeLayout>



                    <LinearLayout
                        android:layout_marginTop="30dp"
                        android:visibility="@{communityCatViewModel.isCommunityUser?View.GONE:View.VISIBLE}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="40dp"
                        android:layout_marginEnd="20dp"
                        android:orientation="vertical">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">


                            <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                style="@style/SubtTitleTextStyle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Hi " />


                            <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                                style="@style/SubtTitleTextStyle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@{communityCatViewModel.name}" />

                        </LinearLayout>

                        <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                            android:layout_marginTop="5dp"
                            style="@style/SubtTitleTextStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@{communityCatViewModel.cat_page_content}" />


                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                            style="@style/ContentTextStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="@{communityCatViewModel.cat_page_subcontent}" />


                    </LinearLayout>



                        <LinearLayout
                            lin_margin_bottom="@{communityCatViewModel.serviceable}"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/categoryList"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:layout_marginTop="20dp"
                                android:layout_marginEnd="5dp" />


                            <com.dailylocally.utilities.fonts.PromotionTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:layout_marginTop="20dp"
                                android:text="Freshly"
                                android:textAlignment="center"
                                android:textColor="@color/very_light_black"
                                android:textSize="@dimen/_65px"
                                android:textStyle="bold" />

                            <com.dailylocally.utilities.fonts.PromotionTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:text="locally"
                                android:textColor="@color/very_light_black"
                                android:textSize="@dimen/_65px"
                                android:textStyle="bold" />

                            <com.dailylocally.utilities.fonts.PromotionTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:text="daily."
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/_65px"
                                android:textStyle="bold" />

                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:layout_marginTop="10dp"
                                android:text="Right Food for all"
                                android:textAlignment="center"
                                text_margin_bottom="@{communityCatViewModel.showRating}"
                                android:textColor="@color/light_black"
                                android:textSize="@dimen/_28px" />
                        </LinearLayout>

                </LinearLayout>

            </androidx.core.widget.NestedScrollView>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/search"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_alignParentTop="true"
            android:background="@color/white"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="10dp"
                app:cardBackgroundColor="@color/search_gray"
                app:cardCornerRadius="5dp"
                app:cardElevation="0dp">

                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical|start"
                    android:onClick="@{()->communityCatViewModel.searchClick()}"
                    android:paddingStart="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:text="Search for fruits, vegetables and essentials"
                    android:textColor="@color/medium_black"
                    android:textSize="@dimen/_15px" />
            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/bottom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:onClick="@{()->communityCatViewModel.ratingClick()}"
                android:visibility="@{communityCatViewModel.showRating?View.VISIBLE:View.GONE}"
                app:cardBackgroundColor="@color/colorPrimary"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp">

                    <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:text="@{communityCatViewModel.ratingTitle}"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_22px" />


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="5dp"
                        android:layout_marginEnd="10dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">


                        <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="@{communityCatViewModel.ratingDate}"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_22px" />


                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginStart="5dp"
                            android:scaleType="fitCenter"
                            android:src="@drawable/ic_l1_arrow"
                            android:tint="@color/white" />
                    </LinearLayout>


                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="10dp"
                        android:onClick="@{()->communityCatViewModel.closeRating()}"
                        android:src="@drawable/ic_close_black_24dp"
                        android:visibility="gone" />

                </RelativeLayout>
            </androidx.cardview.widget.CardView>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/back_unserviceable"
                android:orientation="vertical"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:visibility="@{communityCatViewModel.serviceable?View.GONE:View.VISIBLE}">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                        style="@style/UnserviceableTextStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="10dp"
                        android:gravity="center"
                        android:text="@{communityCatViewModel.unserviceableTitle}"
                        android:textAlignment="center" />

                    <!-- <ImageView
                         android:layout_width="15dp"
                         android:layout_height="15dp"
                         android:layout_alignParentEnd="true"
                         android:layout_marginTop="5dp"
                         android:layout_marginEnd="10dp"
                         android:src="@drawable/ic_close_black_24dp" />-->


                </RelativeLayout>


                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                    style="@style/UnserviceableTextStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:gravity="center"
                    android:text="@{communityCatViewModel.unserviceableSubTitle}"
                    android:textAlignment="center" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:background="@drawable/gradient_bt_back"
                android:orientation="vertical"
                android:visibility="@{communityCatViewModel.updateAvailable?View.VISIBLE:View.GONE}">

                <!--android:background="#9E9E9E"-->

                <!--<View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/medium_gray"/>-->

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="8dp">


                    <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                        android:id="@+id/name"
                        style="@style/SubtTitleTextStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="10dp"
                        android:text="@{communityCatViewModel.updateTitle}" />


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_marginEnd="15dp">

                        <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                            android:id="@+id/later"
                            style="@style/SubtTitleTextStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="@string/later" />


                        <!-- <com.facebook.shimmer.ShimmerFrameLayout
                             android:layout_marginStart="20dp"
                             android:id="@+id/update"
                             app:duration="1000"
                             app:repeat_mode="reverse"
                             android:layout_width="match_parent"
                             android:layout_height="wrap_content">-->


                        <com.dailylocally.utilities.fonts.quicksand.ButtonTextView
                            android:id="@+id/action"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:gravity="center"
                            android:text="@{communityCatViewModel.updateAction}"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_22px"
                            android:visibility="@{communityCatViewModel.update?View.VISIBLE:View.GONE}" />
                        <!-- </com.facebook.shimmer.ShimmerFrameLayout>-->
                    </LinearLayout>


                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:visibility="@{communityCatViewModel.categoryLoading?View.VISIBLE:View.GONE}">

            <!--<com.victor.loading.rotate.RotateLoading
                android:id="@+id/loader"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_centerInParent="true"
                app:loading_color="@color/dl_primary_color"
                app:loading_width="5dp" />-->

            <com.facebook.shimmer.ShimmerFrameLayout
                android:id="@+id/loader"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:duration="1000">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="200dp"
                        android:layout_height="20dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="50dp"
                        android:background="@color/light_gray" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:layout_marginEnd="30dp"
                        android:background="@color/light_gray" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="20dp"
                        android:background="@color/light_gray" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:layout_marginEnd="10dp"
                        android:orientation="horizontal"
                        android:weightSum="2">


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_marginStart="10dp"
                            android:layout_weight="1"
                            android:background="@color/light_gray"
                            android:orientation="horizontal" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_marginStart="10dp"
                            android:layout_weight="1"
                            android:background="@color/light_gray"
                            android:orientation="horizontal" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="10dp"
                        android:orientation="horizontal"
                        android:weightSum="2">


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_marginStart="10dp"
                            android:layout_weight="1"
                            android:background="@color/light_gray"
                            android:orientation="horizontal" />

