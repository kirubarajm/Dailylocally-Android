<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="communityViewModel"
            type="com.dailylocally.ui.joinCommunity.CommunityActivityViewModel" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

  <RelativeLayout
        android:layout_above="@+id/rel_buttons"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <fragment
            android:id="@+id/mapFragment"
            android:name="com.google.android.gms.maps.MapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <RelativeLayout
            android:id="@+id/rel_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:background="@color/transparent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                    <RelativeLayout
                        android:background="@color/transparent"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:paddingTop="@dimen/_10px"
                            android:id="@+id/rel_back"
                            android:layout_width="match_parent"
                            android:layout_height="60dp"
                            android:background="@color/transparent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/img_back"
                                android:layout_width="20dp"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:onClick="@{()->communityViewModel.goBack()}"
                                android:layout_height="20dp"
                                android:background="@color/transparent"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/ic_arrow_back_black_24dp" />


                            <androidx.cardview.widget.CardView
                                android:layout_marginLeft="50dp"
                                app:cardCornerRadius="10dp"
                                android:visibility="@{communityViewModel.register?View.VISIBLE:View.GONE}"
                                android:layout_marginRight="50dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <androidx.appcompat.widget.SearchView
                                    android:id="@+id/search_community"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/white"
                                    android:focusable="false"
                                    app:iconifiedByDefault="false"
                                    app:queryHint="Search for your community or apartment" />

                            </androidx.cardview.widget.CardView>

                        </RelativeLayout>

                    </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:visibility="@{communityViewModel.joinExpandView?View.VISIBLE:View.GONE}"
                android:id="@+id/rel_join_community_expand_view"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:padding="20dp"
                        android:background="@color/white"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                            android:id="@+id/txt_community_name"
                            android:text="Commanders court"
                            android:textSize="@dimen/_15px"
                            android:layout_centerHorizontal="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                            android:textSize="@dimen/_12px"
                            android:layout_below="@+id/txt_community_name"
                            android:layout_centerHorizontal="true"
                            android:id="@+id/txt_location"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <RelativeLayout
                            android:layout_marginTop="5dp"
                            android:id="@+id/rel_2"
                            android:layout_below="@+id/txt_location"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                android:text="Residences"
                                android:id="@+id/txt_residences"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>

                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                android:id="@+id/txt_no_of_apartments"
                                android:layout_alignParentRight="true"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>

                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                android:text="Status"
                                android:layout_marginTop="5dp"
                                android:id="@+id/txt_stat"
                                android:layout_below="@+id/txt_residences"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>

                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                android:id="@+id/txt_status"
                                android:layout_marginTop="5dp"
                                android:layout_below="@+id/txt_no_of_apartments"
                                android:layout_alignParentRight="true"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>

                        </RelativeLayout>

                    </RelativeLayout>

                </androidx.cardview.widget.CardView>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rel_complete_registration"
                android:visibility="@{communityViewModel.completeRegistration?View.VISIBLE:View.GONE}"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:padding="15dp"
                        android:background="@color/white"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:layout_centerHorizontal="true"
                            android:id="@+id/rel_img_reg"
                            android:layout_width="80dp"
                            android:layout_height="80dp">

                            <com.dailylocally.utilities.CircularImageView
                                android:onClick="@{()->communityViewModel.registerCommunityUploadImageClick()}"
                                android:id="@+id/img_registration"
                                android:layout_centerHorizontal="true"
                                profileImage="@{communityViewModel.imageUrl}"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:src="@drawable/ic_group_482" />

                            <ImageView
                                android:onClick="@{()->communityViewModel.closeRegClick()}"
                                android:layout_alignParentEnd="true"
                                android:src="@mipmap/ic_close"
                                android:visibility="@{communityViewModel.flagRemovePicReg?View.VISIBLE:View.GONE}"
                                android:layout_width="15dp"
                                android:layout_height="15dp"/>

                        </RelativeLayout>

                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                            android:text="Upload profile photo"
                            android:layout_below="@+id/rel_img_reg"
                            android:layout_marginTop="5dp"
                            android:id="@+id/txt_upload"
                            android:layout_centerHorizontal="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_below="@+id/txt_upload"
                            android:id="@+id/imput_community_name"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.google.android.material.textfield.TextInputEditText
                                android:hint="Community Name*"
                                android:id="@+id/edt_community_name"
                                android:textSize="@dimen/_15px"
                                android:layout_marginTop="10dp"
                                android:imeOptions="actionNext"
                                android:layout_below="@+id/txt_upload"
                                android:fontFamily="@font/quicksand_regular"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:id="@+id/linear_1"
                            android:layout_below="@+id/imput_community_name"
                            android:weightSum="2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/imput_house_flat_no"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:hint="House/Flat No.*"
                                    android:textSize="@dimen/_15px"
                                    android:imeOptions="actionNext"
                                    android:id="@+id/edt_house_flat_no"
                                    android:fontFamily="@font/quicksand_regular"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>

                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/imput_floor_no_re"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:hint="Floor no*"
                                    android:imeOptions="actionNext"
                                    android:textSize="@dimen/_15px"
                                    android:id="@+id/edt_floor_no_re"
                                    android:fontFamily="@font/quicksand_regular"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_below="@+id/linear_1"
                            android:id="@+id/imput_no_of_apartments"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.google.android.material.textfield.TextInputEditText
                                android:layout_below="@+id/linear_1"
                                android:hint="No of Apartments"
                                android:layout_marginTop="10dp"
                                android:imeOptions="actionNext"
                                android:textSize="@dimen/_15px"
                                android:id="@+id/edt_no_of_apartments"
                                android:fontFamily="@font/quicksand_regular"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_below="@+id/imput_no_of_apartments"
                            android:id="@+id/imput_community_address"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.google.android.material.textfield.TextInputEditText
                                android:hint="Community Address*"
                                android:layout_marginTop="10dp"
                                android:imeOptions="actionDone"
                                android:layout_marginBottom="10dp"
                                android:id="@+id/edt_community_address"
                                android:textSize="@dimen/_15px"
                                android:fontFamily="@font/quicksand_regular"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </com.google.android.material.textfield.TextInputLayout>

                    </RelativeLayout>

                </androidx.cardview.widget.CardView>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rel_join_the_community"
                android:visibility="@{communityViewModel.joinTheCommunity?View.VISIBLE:View.GONE}"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                    <RelativeLayout
                        android:padding="10dp"
                        android:background="@color/white"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:layout_centerHorizontal="true"
                            android:id="@+id/rel_img_join"
                            android:layout_width="80dp"
                            android:layout_height="80dp">

                            <com.dailylocally.utilities.CircularImageView
                                android:onClick="@{()->communityViewModel.joinCommunityUploadImageClick()}"
                                android:id="@+id/img_join"
                                android:layout_centerHorizontal="true"
                                profileImage="@{communityViewModel.imageUrl}"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:src="@drawable/ic_group_482" />

                            <ImageView
                                android:onClick="@{()->communityViewModel.closeClick()}"
                                android:layout_alignParentEnd="true"
                                android:src="@mipmap/ic_close"
                                android:visibility="@{communityViewModel.flagRemovePicReg?View.VISIBLE:View.GONE}"
                                android:layout_width="15dp"
                                android:layout_height="15dp"/>

                        </RelativeLayout>

                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                            android:text="Upload profile photo"
                            android:layout_below="@+id/rel_img_join"
                            android:layout_marginTop="5dp"
                            android:id="@+id/txt_upload1"
                            android:layout_centerHorizontal="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <LinearLayout
                            android:weightSum="2"
                            android:layout_marginTop="30dp"
                            android:layout_marginBottom="10dp"
                            android:orientation="horizontal"
                            android:layout_below="@+id/txt_upload1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:layout_height="wrap_content">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:hint="House/Flat No.*"
                                    android:id="@+id/edt_house"
                                    android:textSize="@dimen/_15px"
                                    android:imeOptions="actionNext"
                                    android:fontFamily="@font/quicksand_regular"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>

                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:layout_height="wrap_content">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:hint="Floor No*"
                                    android:layout_weight="1"
                                    android:id="@+id/edt_floor_no"
                                    android:imeOptions="actionDone"
                                    android:textSize="@dimen/_15px"
                                    android:fontFamily="@font/quicksand_regular"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                    </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

  </RelativeLayout>

        <RelativeLayout
            android:layout_above="@+id/rel_buttons"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.yarolegovich.discretescrollview.DiscreteScrollView
                android:layout_alignParentBottom="true"
                android:visibility="@{communityViewModel.register?View.VISIBLE:View.GONE}"
                android:id="@+id/recycler_community"
                android:layout_marginBottom="30dp"
                adapter="@{communityViewModel.communityItemViewModels}"
                tools:listitem="@layout/list_item_community"
                app:dsv_orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_alignParentBottom="true"
            android:id="@+id/rel_buttons"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/btn_register"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:onClick="@{()->communityViewModel.onRegistrationClick()}"
                android:visibility="@{communityViewModel.register?View.VISIBLE:View.GONE}"
                android:background="@drawable/gradient_bt_back">

                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:paddingStart="15dp"
                    android:paddingTop="2dp"
                    android:paddingEnd="15dp"
                    android:paddingBottom="2dp"
                    android:singleLine="true"
                    android:textAllCaps="true"
                    android:text="Apply for Community registration"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_25px"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/btn_join_comm"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:onClick="@{()->communityViewModel.joinClick()}"
                android:visibility="@{communityViewModel.joinExpandView?View.VISIBLE:View.GONE}"
                android:background="@drawable/gradient_bt_back">

                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:paddingStart="15dp"
                    android:paddingTop="2dp"
                    android:paddingEnd="15dp"
                    android:paddingBottom="2dp"
                    android:singleLine="true"
                    android:textAllCaps="true"
                    android:text="Join Community"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_25px"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/btn_complete_reg"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:onClick="@{()->communityViewModel.completeRegistrationClick()}"
                android:visibility="@{communityViewModel.completeRegistration?View.VISIBLE:View.GONE}"
                android:background="@drawable/gradient_bt_back">

                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:paddingStart="15dp"
                    android:paddingTop="2dp"
                    android:paddingEnd="15dp"
                    android:paddingBottom="2dp"
                    android:singleLine="true"
                    android:textAllCaps="true"
                    android:text="Complete registration"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_25px"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/btn_join_the_comm"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:visibility="@{communityViewModel.joinTheCommunity?View.VISIBLE:View.GONE}"
                android:background="@drawable/gradient_bt_back"
                android:onClick="@{()->communityViewModel.joinTheCommunityClick()}">

                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_margin="5dp"
                    android:paddingStart="15dp"
                    android:paddingTop="2dp"
                    android:paddingEnd="15dp"
                    android:paddingBottom="2dp"
                    android:singleLine="true"
                    android:textAllCaps="true"
                    android:text="Join the community"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_25px"
                    android:textStyle="bold" />

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:visibility="gone"
            android:background="@color/transparent"
            android:id="@+id/rel_view_pager"
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.viewpager.widget.ViewPager
                android:id="@+id/view_pager"
                android:background="@color/transparent"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </RelativeLayout>

        <LinearLayout
            android:visibility="gone"
            android:id="@+id/layoutDots"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dots_height"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/dots_margin_bottom"
            android:gravity="center"
            android:orientation="horizontal">

        </LinearLayout>

        <RelativeLayout
            android:background="#cc1A1A1A"
            android:id="@+id/rel_onboarding_all"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <ImageView
                android:layout_marginTop="160dp"
                android:id="@+id/img_sll"
                android:layout_centerHorizontal="true"
                android:src="@drawable/ic_group_438"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        <RelativeLayout
            android:id="@+id/rel_onboarding_1"
            android:gravity="center"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:visibility="invisible"
                    android:id="@+id/img_1"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/ic_group_438"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                    android:layout_below="@+id/img_1"
                    android:layout_marginTop="40dp"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/txt_1"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_12px"
                    android:text="Join DL Exclusive to enjoy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                    android:layout_below="@+id/txt_1"
                    android:id="@+id/txt_2"
                    android:textSize="@dimen/_12px"
                    android:textColor="@color/white"
                    android:layout_centerHorizontal="true"
                    android:text="An All New Daily Locally Experience"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>


                <LinearLayout
                    android:layout_centerHorizontal="true"
                    android:orientation="vertical"
                    android:layout_below="@+id/txt_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:id="@+id/linear_minimum"
                        android:orientation="horizontal"
                        android:layout_marginTop="40dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                            android:text="&#8226; "
                            android:textColor="@color/white"
                            android:textSize="@dimen/_22px"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                            android:id="@+id/txt_minimum_bskt_value"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_12px"
                            android:layout_centerHorizontal="true"
                            android:text="0 minimum basket value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_free"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                            android:text="&#8226; "
                            android:textColor="@color/white"
                            android:textSize="@dimen/_22px"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                            android:id="@+id/txt_free_delivery"
                            android:textSize="@dimen/_12px"
                            android:textColor="@color/white"
                            android:layout_centerHorizontal="true"
                            android:text="Free delivery"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                            android:text="&#8226; "
                            android:textColor="@color/white"
                            android:textSize="@dimen/_22px"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                            android:layout_below="@+id/txt_free_delivery"
                            android:id="@+id/txt_5"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_12px"
                            android:layout_centerHorizontal="true"
                            android:text="Cash on delivery"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:visibility="gone"
            android:id="@+id/rel_onboarding_2"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <RelativeLayout
