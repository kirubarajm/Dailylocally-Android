<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="android.view.View" />

        <variable
            name="communityViewModel"
            type="com.dailylocally.ui.community.CommunityViewModel" />
    </data>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/gray">

        <LinearLayout
            android:id="@+id/header"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:background="@color/white"
            android:gravity="center"
            android:orientation="vertical">


            <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                style="@style/HeaderTitleTextStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|center_vertical"
                android:text="Community" />

        </LinearLayout>

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/header"
            android:background="@color/gray"
            android:overScrollMode="never">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bottom_curve_stroke_white"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="40dp"
                        android:layout_marginBottom="20dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="10dp"
                            android:orientation="horizontal">


                            <com.dailylocally.utilities.CircularImageView
                                android:id="@+id/circleImage"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:src="@drawable/profile" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginStart="20dp"
                                android:orientation="vertical">

                                <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                    style="@style/SubtTitleTextStyle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Lucy" />


                                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                    style="@style/ContentTextStyle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Hi, Welcome to the Daily Locally community Exclusive club " />


                            </LinearLayout>

                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="40dp"
                            android:layout_marginEnd="20dp"
                            android:orientation="horizontal"
                            android:weightSum="4">


                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical">

                                    <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="200"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="@dimen/_22px" />


                                    <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                        style="@style/ContentTextStyle"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Members" />


                                </LinearLayout>


                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2">


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical">

                                    <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Commander's court"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="@dimen/_22px" />


                                    <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                        style="@style/ContentTextStyle"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Egmore" />


                                </LinearLayout>


                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    android:layout_centerHorizontal="true"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical">

                                    <com.dailylocally.utilities.fonts.quicksand.SubTitleTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="1500"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="@dimen/_22px" />


                                    <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                        style="@style/ContentTextStyle"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="DL credits" />


                                </LinearLayout>


                            </RelativeLayout>
                        </LinearLayout>

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginEnd="5dp"
                        app:cardCornerRadius="10dp"
                        app:cardUseCompatPadding="true">

                        <RelativeLayout
                            android:layout_marginBottom="15dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RelativeLayout
                                android:id="@+id/topConts"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_marginTop="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_alignParentEnd="true"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:layout_marginEnd="10dp"
                                    android:src="@drawable/ic_add_fav" />

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <com.dailylocally.utilities.CircularImageView
                                        imageUrl="@{communityViewModel.icon}"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginStart="10dp" />


                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="10dp"
                                        android:layout_marginEnd="50dp"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_margin="10dp"
                                            android:orientation="vertical">


                                            <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                                                style="@style/HeaderTitleTextStyle"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:text="@{communityViewModel.postTitle}" />


                                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                                style="@style/ContentTextStyle"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="5dp"
                                                android:text="@{communityViewModel.postDate}" />


                                        </LinearLayout>


                                    </LinearLayout>

                                </LinearLayout>

                            </RelativeLayout>


                            <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                android:id="@+id/fullDes"
                                style="@style/ContentTextStyle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/topConts"
                                android:layout_marginStart="20dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginEnd="10dp"
                                android:text="@{communityViewModel.postDes}" />


                            <LinearLayout
                                android:id="@+id/images"
                                android:layout_below="@+id/fullDes"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:visibility="@{communityViewModel.singleImage?View.GONE:View.VISIBLE}"
                                    android:weightSum="2">


                                    <androidx.cardview.widget.CardView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="5dp"
                                        android:layout_weight="1"
                                        app:cardCornerRadius="10dp"
                                        app:cardElevation="0dp">

                                        <ImageView
                                            android:adjustViewBounds="true"
                                            imageUrl="@{communityViewModel.image1}"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content" />

                                    </androidx.cardview.widget.CardView>


                                    <androidx.cardview.widget.CardView
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="5dp"
                                        android:layout_weight="1"
                                        app:cardCornerRadius="10dp"
                                        app:cardElevation="0dp">

                                        <ImageView
                                            android:adjustViewBounds="true"
                                            imageUrl="@{communityViewModel.image2}"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content" />

                                    </androidx.cardview.widget.CardView>


                                </LinearLayout>

                                <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:visibility="@{communityViewModel.singleImage?View.VISIBLE:View.GONE}"
                                    app:cardCornerRadius="10dp"
                                    app:cardElevation="0dp">

                                    <ImageView
                                        imageUrl="@{communityViewModel.image1}"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content" />

                                </androidx.cardview.widget.CardView>

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="30dp"
                                android:layout_below="@+id/images"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentBottom="true"
                                android:layout_gravity="end"
                                android:layout_marginTop="10dp"
                                android:layout_marginEnd="10dp"
                                android:background="@drawable/curve_stroke_rounded"
                                android:visibility="@{communityViewModel.showAction?View.VISIBLE:View.GONE}">

                                <RelativeLayout
                                    android:id="@+id/num_picker"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_centerInParent="true"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginEnd="15dp">


                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:text="@{communityViewModel.actionText}"
                                        android:textAlignment="center"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="14sp" />


                                </RelativeLayout>


                            </RelativeLayout>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="5dp"
                        android:orientation="horizontal"
                        android:weightSum="2">


                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1">

                            <androidx.cardview.widget.CardView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:cardCornerRadius="10dp"
                                app:cardUseCompatPadding="true">

                                <ImageView
                                    imageUrl="@{communityViewModel.eventImageUrl}"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content" />

                            </androidx.cardview.widget.CardView>
                            <!-- <LinearLayout
                                 android:layout_margin="10dp"
                                 android:layout_width="match_parent"
                                 android:layout_height="wrap_content"
