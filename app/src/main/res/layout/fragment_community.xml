                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:cardCornerRadius="10dp"
                                        app:cardUseCompatPadding="true">


                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">

                                            <LinearLayout
                                                android:id="@+id/aboutContent"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_margin="10dp"
                                                android:orientation="vertical">


                                                <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityViewModel.aboutUsTitle}"
                                                    android:textColor="@color/medium_black"
                                                    android:textSize="@dimen/_25px" />


                                                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                                    style="@style/ContentTextStyle"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginTop="5dp"
                                                    android:text="@{communityViewModel.aboutUsDes}" />


                                            </LinearLayout>

                                            <ImageView
                                                android:layout_width="30dp"
                                                android:layout_height="wrap_content"
                                                android:layout_below="@+id/aboutContent"
                                                android:layout_alignParentEnd="true"
                                                android:layout_margin="10dp"
                                                android:adjustViewBounds="true"
                                                android:src="@drawable/update" />


                                        </RelativeLayout>

                                    </androidx.cardview.widget.CardView>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">


                                    <androidx.cardview.widget.CardView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:cardCornerRadius="10dp"
                                        app:cardUseCompatPadding="true">

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">

                                            <LinearLayout
                                                android:id="@+id/sneakContent"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_margin="10dp"
                                                android:orientation="vertical">


                                                <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="@{communityViewModel.sneakTitle}"
                                                    android:textColor="@color/medium_black"
                                                    android:textSize="@dimen/_25px" />


                                                <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                                    style="@style/ContentTextStyle"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginTop="5dp"
                                                    android:text="@{communityViewModel.sneakDes}" />


                                            </LinearLayout>

                                            <ImageView
                                                android:layout_width="30dp"
                                                android:layout_height="wrap_content"
                                                android:layout_below="@+id/sneakContent"
                                                android:layout_alignParentEnd="true"
                                                android:layout_margin="10dp"
                                                android:adjustViewBounds="true"
                                                android:src="@drawable/update" />


                                        </RelativeLayout>

                                    </androidx.cardview.widget.CardView>

                                </LinearLayout>

                            </LinearLayout>


                            <!-- <LinearLayout
                                 android:layout_margin="10dp"
                                 android:layout_width="match_parent"
                                 android:layout_height="wrap_content"
                                 android:orientation="vertical">


                                 <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                                     style="@style/HeaderTitleTextStyle"
                                     android:layout_width="wrap_content"
                                     android:layout_height="wrap_content"
                                     android:text="@{communityViewModel.}" />


                                 <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                     style="@style/ContentTextStyle"
                                     android:layout_width="wrap_content"
                                     android:layout_height="wrap_content"
                                     android:layout_marginTop="5dp"
                                     android:text="Hi, Welcome to the Daily Locally community Exclusive club " />


                             </LinearLayout>-->

                        </RelativeLayout>


                        <RelativeLayout

                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">


                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                app:cardCornerRadius="10dp"
                                app:cardUseCompatPadding="true">


                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <ImageView
                                        android:layout_width="40dp"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/whatsContent"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="50dp"
                                        android:adjustViewBounds="true"
                                        android:src="@drawable/update" />

                                    <LinearLayout
                                        android:id="@+id/whatsContent"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="10dp"
                                        android:orientation="vertical">


                                        <com.dailylocally.utilities.fonts.quicksand.TitleTextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="@{communityViewModel.whatsTitle}"
                                            android:textColor="@color/medium_black"
                                            android:textSize="@dimen/_25px" />


                                        <com.dailylocally.utilities.fonts.quicksand.ContentTextView
                                            style="@style/ContentTextStyle"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="5dp"
                                            android:text="@{communityViewModel.whatsDes}" />


                                    </LinearLayout>

                                </RelativeLayout>

                            </androidx.cardview.widget.CardView>
                        </RelativeLayout>
                    </LinearLayout>




                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        postadapter="@{communityViewModel.getSocialActivities}"
                        android:id="@+id/postList"/>



                </LinearLayout>

            </LinearLayout>

        </androidx.core.widget.NestedScrollView>


